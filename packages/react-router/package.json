{
  "name": "@auth-kit/react-router",
  "version": "3.1.3",
  "description": "React Router Support for React-auth-kit",
  "keywords": [
    "react",
    "router",
    "privateroute",
    "auth"
  ],
  "author": "Arkadip Bhattacharya <hi@arkadip.dev>",
  "homepage": "https://authkit.arkadip.dev/",
  "license": "MIT",
  "source": "/src/index.ts",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "files": [
    "*.js",
    "*.d.ts"
  ],
  "exports": {
    ".": "./index.js",
    "./AuthOutlet": "./AuthOutlet.js",
    "./RequireAuth": "./RequireAuth.js",
    "./route": "./route.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-auth-kit/react-auth-kit.git"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:build": "tsc --project tsconfig.json && babel src --out-dir . --extensions \".tsx,.ts,.js,.jsx\"",
    "test": "jest",
    "lint": "eslint src",
    "test:watch": "jest --watch"
  },
  "bugs": {
    "url": "https://github.com/react-auth-kit/react-auth-kit/issues"
  },
  "peerDependencies": {
    "react": ">=16.8.0 || ^17.x || ^18.x",
    "react-router": ">=6.x"
  },
  "devDependencies": {
    "@types/node": "22.10.5",
    "@types/react": "18.3.18",
    "@types/react-router-dom": "5.3.3",
    "react": "18.3.1",
    "react-router": "6.28.0",
    "react-router-dom": "^6.22.3"
  },
  "gitHead": "8326f5a8a331e0eacc59bbc4aab7da0671b86a84",
  "dependencies": {
    "react-auth-kit": "^3.1.3"
  }
}
