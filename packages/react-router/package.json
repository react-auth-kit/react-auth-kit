{
  "name": "@auth-kit/react-router",
  "version": "4.0.1-alpha.0",
  "description": "React Router Support for React-auth-kit",
  "keywords": [
    "react",
    "router",
    "privateroute",
    "auth"
  ],
  "author": "Arkadip Bhattacharya <hi@arkadip.dev>",
  "homepage": "https://authkit.arkadip.dev/",
  "license": "MIT",
  "source": "/src/index.ts",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "directories": {
    "src": "src",
    "lib": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist",
    "*.js",
    "*.d.ts"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./AuthOutlet": "./dist/AuthOutlet.js",
    "./RequireAuth": "./dist/RequireAuth.js",
    "./route": "./dist/route.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-auth-kit/react-auth-kit.git"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:build": "rimraf ./dist && mkdirp ./dist && tsc --project tsconfig.json && babel src --out-dir ./dist --extensions \".tsx,.ts,.js,.jsx\"",
    "test": "jest",
    "lint": "eslint src",
    "test:watch": "jest --watch"
  },
  "bugs": {
    "url": "https://github.com/react-auth-kit/react-auth-kit/issues"
  },
  "devDependencies": {
    "react-router": "7.7.1",
    "react-router-dom": "7.7.1"
  },
  "dependencies": {
    "react-auth-kit": "^4.0.1-alpha.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0 || ^17.x || ^18.x || ^19.x",
    "react-router": ">=6.x"
  },
  "gitHead": "8326f5a8a331e0eacc59bbc4aab7da0671b86a84"
}
