{
  "name": "react-auth-kit",
  "version": "3.0.0-alpha.23",
  "description": "Authentication Library for React, which makes Token based auth very easy",
  "keywords": [
    "react",
    "auth",
    "authentication",
    "jwt",
    "jsonwebtoken",
    "token"
  ],
  "author": "Arkadip Bhattacharya <hi@arkadip.dev>",
  "homepage": "https://authkit.arkadip.dev",
  "license": "MIT",
  "source": "src/index.tsx",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "directories": {
    "src": "src",
    "test": "__tests__"
  },
  "files": [
    "higherOrderComponents",
    "hooks",
    "utils",
    "*.js",
    "*.d.ts"
  ],
  "exports": {
    ".": "./index.js",
    "./AuthContext": "./AuthContext.js",
    "./AuthProvider": "./AuthProvider.js",
    "./PrivateRoute": "./PrivateRoute.js",
    "./errors": "./errors.js",
    "./hooks/*": "./hooks/*.js",
    "./higherOrderComponents/*": "./higherOrderComponents/*.js",
    "./utils/*": "./utils/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-auth-kit/react-auth-kit.git"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:build": "tsc --project tsconfig.json && babel --config-file ./config/babel.config.js src --out-dir . --extensions \".tsx,.ts,.js,.jsx\"",
    "test": "run-s test:*",
    "test:unit": "jest --config ./config/jest.config.js",
    "test:lint": "eslint src/**/*.ts src/**/*.tsx --fix",
    "test:unit:watch": "jest --watch"
  },
  "bugs": {
    "url": "https://github.com/react-auth-kit/react-auth-kit/issues"
  },
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@babel/core": "7.18.2",
    "@babel/plugin-transform-optional-catch-binding": "7.22.11",
    "@babel/plugin-transform-runtime": "7.18.2",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-react": "7.17.12",
    "@babel/preset-typescript": "7.17.12",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.5.5",
    "@types/js-cookie": "3.0.4",
    "@types/node": "18.18.3",
    "@types/react": "18.2.23",
    "@types/react-dom": "18.2.8",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "eslint": "8.50.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-jest": "27.4.2",
    "eslint-plugin-react": "7.33.2",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.4.3",
    "ts-jest": "29.1.1"
  },
  "dependencies": {
    "js-cookie": "^3.0.1",
    "rxjs": "^7.8.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0 || ^17.x || ^18.x",
    "react-dom": ">=16.8.0 || ^17.x || ^18.x",
    "react-router-dom": ">=6.x"
  },
  "gitHead": "6c4618ae048bee06214a382df54dc63e8378575f"
}
